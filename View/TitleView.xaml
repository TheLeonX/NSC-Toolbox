<Window xmlns:Controls="clr-namespace:NSC_Toolbox.Controls"  x:Class="NSC_Toolbox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NSC_Toolbox"
        xmlns:ui="http://schemas.modernwpf.com/2019"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"  
        xmlns:xcdg="http://schemas.xceed.com/wpf/xaml/datagrid"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        xmlns:properties="clr-namespace:NSC_Toolbox.Properties"
        ui:WindowHelper.UseModernWindowStyle="True"
        mc:Ignorable="d"
        xmlns:converter="clr-namespace:NSC_Toolbox.Converter"
        
        Title="{DynamicResource m_Title}"  Height="800" Width="900">
    <Window.Background>
        <ImageBrush ImageSource="{DynamicResource ResourceKey=TitleBackground}" Stretch="UniformToFill"/>
    </Window.Background>
    <Window.Resources>
        <Style x:Key="NoChromeButton" TargetType="{x:Type Button}">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="Foreground" Value="Transparent"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Padding" Value="1"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Grid x:Name="Chrome" Background="{TemplateBinding Background}" SnapsToDevicePixels="true">
                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsPressed" Value="True">
                                <Trigger.EnterActions>
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <DoubleAnimation
                                                Storyboard.TargetProperty="MinWidth"
                                                From="330"
                                                To="340"
                                                Duration="0:0:0.1"
                                                AutoReverse="True"/>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </Trigger.EnterActions>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
    <Viewbox Stretch="{Binding Source={x:Static properties:Settings.Default}, Path=StretchMode}">
        <Grid>
            <Grid ShowGridLines="False" Height="1000" Width="1000"  Visibility="{Binding MainWindowVisibility}" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="2*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="6*"/>
                    <RowDefinition Height="70"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="50"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Border Grid.Row="0" BorderBrush="White" BorderThickness="0,0,0,1"/>
                    <Menu VerticalAlignment="Stretch" FontFamily="{StaticResource NarutoFont}" >
                        <MenuItem Header="{DynamicResource m_mainWindow_006}" Foreground="{DynamicResource Toolbox_button_text_color_1}">
                            <MenuItem Header="{DynamicResource m_mainWindow_010}" Command="{Binding InstallModdingAPICommand}" Foreground="{DynamicResource Toolbox_button_text_color_1}"/>
                            <MenuItem Header="{DynamicResource m_mainWindow_011}" Command="{Binding DeleteModdingAPICommand}" Foreground="{DynamicResource Toolbox_button_text_color_1}"/>

                        </MenuItem>
                        <MenuItem Foreground="{DynamicResource Toolbox_button_text_color_1}" Command="{Binding VisitModdingGroupCommand}">
                            <MenuItem.Header>
                                <StackPanel Orientation="Horizontal">
                                    <Path  VerticalAlignment="Center" Stroke="#FF4B1C67" Fill="{DynamicResource Toolbox_button_text_color_1}"  Data="{DynamicResource Discord_logo}"/>
                                    <TextBlock Margin="5,0,0,0" TextAlignment="Left" VerticalAlignment="Center" Text="{DynamicResource m_mainWindow_007}"/>
                                </StackPanel>
                            </MenuItem.Header>
                        </MenuItem>
                        <MenuItem Header="{DynamicResource m_mainWindow_012}" Command="{Binding BoostyCommand}" Foreground="{DynamicResource Toolbox_button_text_color_1}"/>
                        <MenuItem Header="{DynamicResource m_mainWindow_008}" Command="{Binding CreditsCommand}" Foreground="{DynamicResource Toolbox_button_text_color_1}"/>
                    </Menu>
                    <Grid Grid.Row="1" Margin="20" Background="{DynamicResource Toolbox_button_background_color_1}" Visibility="{Binding CharacterManagementVisibility}">
                        <Button MouseEnter="CharacodeEditor_MouseEnter" Command="{Binding CharacodeRunButtonCommand}" Grid.Column="1" Grid.Row="1" VerticalAlignment="Stretch" HorizontalContentAlignment="Center" HorizontalAlignment="Stretch" MinHeight="50" Background="{DynamicResource Toolbox_button_background_color_2}" BorderBrush="{DynamicResource Toolbox_button_border_color_2}" FontFamily="{StaticResource NarutoFont}" Foreground="{DynamicResource Toolbox_button_text_color_1}" FontSize="14">
                            <TextBlock TextWrapping="WrapWithOverflow" VerticalAlignment="Center" TextAlignment="Center" Text="{DynamicResource m_mainWindowTool_001}"/>
                        </Button>
                        <Button MouseEnter="DuelPlayerParamEditor_MouseEnter" Command="{Binding DuelPlayerParamEditorRunButtonCommand}" Grid.Column="1" Grid.Row="3" VerticalAlignment="Stretch" HorizontalContentAlignment="Center" HorizontalAlignment="Stretch" MinHeight="50" Background="{DynamicResource Toolbox_button_background_color_2}" BorderBrush="{DynamicResource Toolbox_button_border_color_2}" FontFamily="{StaticResource NarutoFont}" Foreground="{DynamicResource Toolbox_button_text_color_1}" FontSize="14">
                            <TextBlock TextWrapping="WrapWithOverflow" VerticalAlignment="Center" TextAlignment="Center" Text="{DynamicResource m_mainWindowTool_002}"/>
